{
  "name": "Argus-frontend",
  "version": "1.0.3",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "v1",
    "@devexpress/dx-react-core": "*",
    "@devexpress/dx-react-scheduler": "*",
    "@devexpress/dx-react-scheduler-material-ui": "*",
    "@material-ui/core": "latest",
    "@material-ui/icons": "*",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "latest",
    "@types/classnames": "^2.2.11",
    "@types/date-fns": "latest",
    "@types/jest": "^26.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-outside-click-handler": "^1.3.0",
    "@types/react-router-dom": "*",
    "@types/react-select": "*",
    "@types/react-table": "^6.0.0",
    "axios": "*",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "lint-staged": "^10.5.3",
    "react": "*",
    "react-cookie": "*",
    "react-dom": "*",
    "react-outside-click-handler": "^1.3.0",
    "react-router-dom": "*",
    "react-scripts": "*",
    "react-select": "*",
    "react-table": "^6.10.0",
    "typescript": "*"
  },
  "scripts": {
    "start": "cross-env HTTPS=true PORT=4443 react-scripts start",
    "dockerstart": "cross-env HTTPS=false PORT=8080 react-scripts start",
    "debug": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src --ext .ts,.tsx,.js",
    "test": "react-scripts test",
    "format": "prettier --write src",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "*",
    "husky": "^4.3.8",
    "prettier": "*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "pre-push": "npx lint-staged",
      "...": "..."
    }
  },
  "lint-staged": {
    "*": "npm run lint"
  }
}
